<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

	<servlet>
		<servlet-name>getAllFilms</servlet-name>
		<servlet-class>coreservlets.getAllFilms</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getAllFilms</servlet-name>
		<url-pattern>/getAllFilms</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>getFilm</servlet-name>
		<servlet-class>coreservlets.getFilm</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getFilm</servlet-name>
		<url-pattern>/getFilm</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>insertFilm</servlet-name>
		<servlet-class>coreservlets.insertFilm</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insertFilm</servlet-name>
		<url-pattern>/insertFilm</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deleteFilm</servlet-name>
		<servlet-class>coreservlets.deleteFilm</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteFilm</servlet-name>
		<url-pattern>/deleteFilm</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>createAllFilms</servlet-name>
		<servlet-class>coreservlets.createAllFilms</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>createAllFilms</servlet-name>
		<url-pattern>/createAllFilms</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>indexCLOUD.html</welcome-file>
	</welcome-file-list>
</web-app>
